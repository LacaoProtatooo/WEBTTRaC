{
  "expo": {
    "name": "WEBT-TRaC",
    "slug": "device",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/webttrac_logo_bgrm.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/webttrac_logo_bgrm.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "BACKEND_URL": "https://webttrac.onrender.com",
      "eas": {
        "projectId": "9318ce3a-b5b0-461a-98bf-1cca498a078e"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/webttrac_logo_bgrm.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./google-services.json",
      "package": "com.company.tricyclemod",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "foregroundService": {
        "notificationTitle": "WEBT-TRaC tracking",
        "notificationBody": "Background location running"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBE_y3W7JmPpbZVpl_uPFV0wSPOxInoAI8"
        }
      },
      "build": {
        "production": {
          "autoIncrement": true,
          "android": {
            "buildType": "apk"
          }
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-google-signin/google-signin",
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": true,
          "android": {
            "enableFTS": false,
            "useSQLCipher": false
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow WEBT-TRaC to access your camera",
          "microphonePermission": "Allow WEBT-TRaC to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-build-properties",
        {}
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/webttrac_logo_bgrm.png",
          "color": "#ffffff"
        }
      ]
    ],
    "owner": "protatooolacao"
  }
}
